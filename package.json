{
  "name": "globle",
  "version": "1.6.5",
  "description": "Use your geography knowledge to figure out the mystery country in as few guesses as possible!",
  "packageManager": "pnpm@10.22.0",
  "scripts": {
    "dev": "vite build && wrangler pages dev dist",
    "dev:watch": "concurrently \"vite build --watch --outDir dist\" \"wrangler pages dev dist --live-reload\"",
    "build": "vite build",
    "serve": "vite preview",
    "vitest": "vitest",
    "test:puppeteer": "vitest run --environment node --dir e2eTests",
    "test:puppeteer:list": "vitest list --dir e2eTests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deploy": "vite build && wrangler pages deploy dist",
    "test-distance": "tsx scripts/testDistance.ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@playwright/test": "^1.47.2",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/crypto-js": "^4.1.1",
    "@types/d3": "^7.4.0",
    "@types/google.accounts": "^0.0.2",
    "@types/node": "^24.3.0",
    "@types/ua-parser-js": "^0.7.36",
    "autoprefixer": "^10.4.13",
    "concurrently": "^9.2.0",
    "dotenv": "^16.0.3",
    "happy-dom": "^17.4.4",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.19",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "puppeteer": "^24.4.0",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-pwa": "^1.0.2",
    "vite-plugin-solid": "^2.11.8",
    "vitest": "^3.2.4",
    "wrangler": "^4.47.0",
    "zod": "^4.1.11"
  },
  "dependencies": {
    "@solidjs/router": "^0.15.3",
    "crypto-js": "^4.1.1",
    "d3": "^7.6.1",
    "dayjs": "^1.11.6",
    "fuse.js": "^6.6.2",
    "globe.gl": "^2.26.10",
    "google-auth-library": "^8.7.0",
    "i18next": "^22.0.6",
    "jose": "^4.15.4",
    "jwt-decode": "^3.1.2",
    "solid-js": "^1.9.9",
    "spherical-geometry-js": "^3.0.0",
    "tiny-invariant": "^1.3.1",
    "ua-parser-js": "^1.0.32"
  }
}
